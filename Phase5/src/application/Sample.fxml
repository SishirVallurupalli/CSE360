<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Login">
   <center>
      <AnchorPane minWidth="-Infinity" prefHeight="600.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="142.0" layoutY="236.0" text="Username" />
            <Label layoutX="142.0" layoutY="265.0" text="Password" />
            <TextField fx:id="username" layoutX="210.0" layoutY="232.0" />
            <TextField fx:id="passwordText" layoutX="210.0" layoutY="261.0" />
            <Button fx:id="button" layoutX="259.0" layoutY="312.0" mnemonicParsing="false" onAction="#login" text="Log In" />
            <Label fx:id="wrongLogIn" layoutX="270.0" layoutY="351.0" />
            <Button fx:id="resetPassword" layoutX="118.0" layoutY="312.0" mnemonicParsing="false" onAction="#ResetPassword" text="Reset Passowrd" />
            <Button fx:id="signUp" layoutX="345.0" layoutY="312.0" mnemonicParsing="false" onAction="#SignUp" text="Sign Up" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
