<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CreateAccount">
   <center>
      <AnchorPane prefHeight="600.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="197.0" layoutY="242.0" text="Username" />
            <Label layoutX="185.0" layoutY="271.0" text="New Password" />
            <TextField fx:id="usernameText" layoutX="265.0" layoutY="238.0" />
            <TextField fx:id="passwordText" layoutX="265.0" layoutY="267.0" />
            <Button fx:id="SignUp" layoutX="303.0" layoutY="332.0" mnemonicParsing="false" onAction="#SignUp" text="Sign Up" />
            <Label fx:id="wrongLogIn" layoutX="325.0" layoutY="357.0" />
            <TextField fx:id="empCodeText" layoutX="265.0" layoutY="300.0" />
            <Label layoutX="185.0" layoutY="304.0" text="Special Code" />
            <Button fx:id="goBack" layoutX="190.0" layoutY="332.0" mnemonicParsing="false" onAction="#GoBack" text="Go Back" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
